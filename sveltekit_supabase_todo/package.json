		"prisma": "^4.10.1",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-katex": "^6.0.2",
		"rehype-katex-svelte": "^1.1.2",
		"rehype-slug": "^5.1.0",
		"remark-abbr": "^1.4.1",
		"remark-math": "^3.0.1",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^5.0.0",
		"tailwindcss": "^3.2.6",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
